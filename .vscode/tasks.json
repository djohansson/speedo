{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "FastBuild (Cache)",
			"type": "shell",
			"command": "fbuild",
			"args": [
				"-cache",
				//"all"
				"debug"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "FastBuild (Clean, Cache)",
			"type": "shell",
			"command": "fbuild",
			"args": [
				"-clean",
				"-cache",
				"all"
			],
			"group": "build"
		},
		{
			"label": "FastBuild (Clean, No Cache)",
			"type": "shell",
			"command": "fbuild",
			"args": [
				"-clean",
				"all"
			],
			"group": "build"
		}
	]
}
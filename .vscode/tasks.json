{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "FastBuild (Cache)",
			"type": "shell",
			"command": "pwsh.exe -c '& {. ./env.ps1 ; Invoke-DevEnv fbuild all -cache}'",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"options": {
				"env": { "Path": "${env:Path}" }
			}
		},
		{
			"label": "FastBuild (Clean, Cache)",
			"type": "shell",
			"command": "pwsh.exe -c '& {. ./env.ps1 ; Invoke-DevEnv fbuild all -clean -cache}'",
			"group": "build",
			"options": {
				"env": { "Path": "${env:Path}" }
			}
		},
		{
			"label": "FastBuild (Clean, No Cache)",
			"type": "shell",
			"command": "pwsh.exe -c '& {. ./env.ps1 ; Invoke-DevEnv fbuild all -clean}'",
			"group": "build",
			"options": {
				"env": { "Path": "${env:Path}" }
			}
		}
	]
}
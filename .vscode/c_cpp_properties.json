{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${env:VULKAN_SDK}/Include",
                "${workspaceRoot}/**"
            ],
            "defines": [
                "__WINDOWS__",
                "_WIN32",
                "_WIN64",
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "_CRT_SECURE_NO_WARNINGS",
                "_WIN32_LEAN_AND_MEAN",
                "NOMINMAX",
                "ENABLE_HLSL",
                "ENABLE_OPT=0",
                "AMD_EXTENSIONS",
                "NV_EXTENSIONS",
            ],
            "intelliSenseMode": "msvc-x64",
            "windowsSdkVersion": "10.0.18362.0",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${env:VULKAN_SDK}/Include",
                    "${workspaceRoot}"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}
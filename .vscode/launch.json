				{ "name": "PATH", "value": "${env:PATH};${workspaceFolder}/build.vcpkg/x64-windows-clang/bin"}
			],
			"symbolOptions": {
				"searchPaths": [],
				"searchMicrosoftSymbolServer": true,
				"cachePath": "${env:TEMP}/symbolcache",
				"moduleFilter": {
					"mode": "loadAllButExcluded",
					"excludedModules": []
				}
			},
			"console": "internalConsole",
			"logging": {
				"exceptions": true,
				"engineLogging": false,
				"processExit": true,
				"programOutput": true,
				"moduleLoad": true,
				"trace": true,

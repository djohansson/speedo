			],
			"targetArchitecture": "arm64",
			"externalConsole": false,
			"MIMode": "lldb"
		},
		{
			"name": "(Windows) debug/client",
			"type": "cppvsdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build.output/debug/client.exe",
			"args": ["-u ${userHome}"],
			"cwd": "${workspaceFolder}",

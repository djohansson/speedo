{
  "name": "speedo",
  "version": "0.0.1",
  "features": {
    "toolchain": {
      "description": "LLVM Toolchain",
      "dependencies": [
        {
          "name": "llvm",
          "default-features": false,
          "features": [
            "clang",
            "clang-tools-extra",
            "compiler-rt",
            "default-targets",
            "libcxx",
            "lld"
          ]
        },
        {
          "name": "llvm",
          "default-features": false,
          "features": [ "libcxxabi" ],
          "platform": "!windows"
        }
      ]
    },
    "common": {
      "description": "Common Dependencies for both Client and Server",
      "dependencies": [
        {
          "name": "vcpkg-cmake",
          "host": true
        },
        {
          "name": "vcpkg-tool-ninja",
          "host": true
        },
        "cargs",
        "concurrentqueue",
        "cppzmq",
        "cpptrace",
        "glaze",
        {
          "name": "mimalloc",
          "features": [
            "override",
            "secure"
          ]
        },
        "mio",
        "picosha2",
        "sha-intrinsics",
        "stduuid",
        "tl-function-ref",
        "tracy",
        "unordered-dense",
        "xxhash",
        "zpp-bits"
      ]
    },
    "client": {
      "description": "Game Client Executable",
      "dependencies": [
        {
          "name": "speedo",
          "features": [ "common" ]
        },
        "ade",
        "cgltf",
        "glfw3",
        {
          "name": "glfw3",
          "default-features": false,
          "features": [ "wayland" ],
          "platform": "linux"
        },
        "glm",
        {
          "name": "imgui",
          "features": [
            "glfw-binding",
            "vulkan-binding"
          ]
        },
        "nativefiledialog-extended",
        "shader-slang",
        "stb",
        "tinyobjloader",
        {
          "name": "tracy",
          "features": [ "gui-tools" ]
        },
        "vulkan",
        "vulkan-headers",
        "vulkan-loader",
        "vulkan-memory-allocator",
        "vulkan-utility-libraries",
        "vulkan-validationlayers"
      ]
    },
    "server": {
      "description": "Game Server Executable",
      "dependencies": [
        {
          "name": "speedo",
          "features": [ "common" ]
        }
      ]
    }
  },
  "builtin-baseline": "66c0373dc7fca549e5803087b9487edfe3aca0a1"
}
